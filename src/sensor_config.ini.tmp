# Values for InfluxDB
# will create DB if it doesnt already exist
[InfluxClient]
Host: localhost
Port: 8086
User: mdtuser
Password: pwd
DatabaseName: sensor_test_database

# For Channels 0-7, set the measurement name. If you want the conversion done before the data is sent to influxdb, then set SendRawValue to False and set the 
# range of the sensor with SensorMin and SensorMax

[ChannelZero]
SendRawValue: True
#SensorMin:
#SensorMax:
## MeasurementName will default to the pin number when converting values on client level
MeasurementName: Pin_0

[ChannelOne]
SendRawValue: True
#SensorMin:
#SensorMax:
## MeasurementName will default to the pin number when converting values on client level
MeasurementName: Pin_1


[ChannelTwo]
SendRawValue: True
#SensorMin:
#SensorMax:
## MeasurementName will default to the pin number when converting values on client level
MeasurementName: Pin_2


[ChannelThree]
SendRawValue: True
#SensorMin:
#SensorMax:
## MeasurementName will default to the pin number when converting values on client level
MeasurementName: Pin_3

[ChannelFour]
SendRawValue: True
#SensorMin:
#SensorMax:
## MeasurementName will default to the pin number when converting values on client level
MeasurementName: Pin_4

[ChannelFive]
SendRawValue: True
#SensorMin:
#SensorMax:
## MeasurementName will default to the pin number when converting values on client level
MeasurementName: Pin_5

[ChannelSix]
SendRawValue: True
#SensorMin:
#SensorMax:
## MeasurementName will default to the pin number when converting values on client level
MeasurementName: Pin_6

[ChannelSeven]
SendRawValue: True
#SensorMin:
#SensorMax:
## MeasurementName will default to the pin number when converting values on client level
MeasurementName: Pin_7

[Etc]
# Reference voltage Used on the MCP3008 chip, more than likely 3300 or 5000
ReferenceVoltage:3300

# Software SPI Pins used for MCP3008

SPICLK : 18
SPIMISO : 23
SPIMOSI : 24
SPICS : 25

